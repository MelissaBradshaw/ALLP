            <a:documentation>describes the status of a document either currently or, when associated
               with a dated element, at the time indicated. Sample values include: 1] approved; 2]
               candidate; 3] cleared; 4] deprecated; 5] draft; 6] embargoed; 7] expired; 8] frozen;
               9] galley; 10] proposed; 11] published; 12] recommendation; 13] submitted; 14]
               unfinished; 15] withdrawn</a:documentation>
            <data type="token">
               <param name="pattern">[^\p{C}\p{Z}]+</param>
            </data>
         </attribute>
      </optional>
   </define>
   <define name="tei_att.global.responsibility.attributes">
      <ref name="tei_att.global.responsibility.attribute.cert"/>
      <ref name="tei_att.global.responsibility.attribute.resp"/>
